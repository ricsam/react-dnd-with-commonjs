{
  "name": "@react-dnd/asap",
  "version": "6.0.0",
  "description": "High-priority task queue for Node.js and browsers",
  "keywords": [
    "event",
    "task",
    "queue"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist/",
    "build_types": "tsc -b .",
    "build_esm": "swc -C module.type=es6 -d dist/esm src/",
    "build_commonjs": "swc -C module.type=commonjs -d dist/cjs src/",
    "add_pkgjson": "node ../../scripts/createDistPackageJson.mjs",
    "build": "run-s clean build_types build_esm build_commonjs add_pkgjson"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-dnd/react-dnd"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.168",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^4.6.3"
  }
}
